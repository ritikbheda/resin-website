    "schemes": [
      "https",
      "http"
    ],
    "paths": {
      "/status": {
        "get": {
          "description": "Multiple status values can be provided with comma separated strings",
          "operationId": "findPetsByStatus",
          "parameters": [
            {
                "name": "id",
                "in": "query",
                "type": "integer",
                "required": false
            
            }
          ],
          "responses": {
            "200": {
              "description": "successful operation",
              "schema": {
                "type": "string",
                "example": "ok"
              }
            }
          }
        }
      },
      "/contact": {
        "get": {
          "description": "contact company page",
          "operationId": "getContact",
          "responses": {
            "200": {
              "description": "successful operation",
              "schema": {
                "type": "string",
                "example": "ok"
              }
            }
          }
        }
      },
      "/collection": {
        "get": {
          "description": "opens page for containing all items",
          "operationId": "collection",
          "parameters":[],
          "responses": {
            "200": {
              "description": "successful operation",
              "schema": {
                "type": "string",
                "example": "ok"
              }
            }
          }
        }
      },
      "/collection/{id}": {
        "get": {
          "description": "opens page for each item",
          "operationId": "collection/{id}",
          "parameters":[
              {
                  "name":"id",
                  "type":"integer",
                  "required":true,
                  "in":"path"
              }
          ],
          "responses": {
            "200": {
              "description": "successful operation",
              "schema": {
                "type": "string",
                "example": "ok"
              }
            }
          }
        }
      }

    }      
